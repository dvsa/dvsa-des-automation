{
  "author": "DVSA",
  "license": "UNLICENSED",
  "name": "dvsa-des-automation",
  "version": "1.0.0",
  "description": "WDIO DES Automation",
  "homepage": "",
  "keywords": [
    "wdio",
    "webdriver",
    "webdriverio",
    "cucumber",
    "test",
    "boilerplate",
    "selenium"
  ],
  "engines": {
    "node": ">=18.11.0 || <19.0.0",
    "npm": ">=8 || <9"
  },
  "scripts": {
    "test": "run-s test:*",
    "scanRepo": "git secrets --scan && git log -p -n 15 | scanrepo",
    "test:lint": "eslint 'src/**/*.ts' 'test/**/*.js!(setup.js)'",
    "wdio:des:ap": "npm run copy-shared && wdio run ./config/wdio.DES-Appium.conf.ts",
    "wdio:des:ap:test": "npm run copy-shared && wdio run ./config/wdio.DES-Appium.conf.ts --cucumberOpts.tagExpression='@test'",
    "wdio:des:bs": "npm run copy-shared && wdio run ./config/wdio.DES-BrowserStack.conf.ts",
    "wdio:des:ap:report": "npm run copy-shared && npm run report:clean && wdio run ./config/wdio.DES-Appium.conf.ts",
    "wdio:des:ap:test:report": "npm run copy-shared && npm run report:clean && wdio run ./config/wdio.DES-Appium.conf.ts --cucumberOpts.tagExpression='@test'",
    "wdio:des:bs:report": "npm run copy-shared && npm run report:clean && wdio run ./config/wdio.DES-BrowserStack.conf.ts",
    "wdio:des:bs:test:report": "npm run copy-shared && npm run report:clean && wdio run ./config/wdio.DES-BrowserStack.conf.ts --cucumberOpts.tagExpression='@test'",
    "report": "allure generate --clean ./reports/allure-results/ -o ./reports/allure-report && allure open ./reports/allure-report",
    "s3report": "allure generate --clean ./reports/allure-results/ -o ./reports/allure-report",
    "report:clean": "rimraf ./reports/allure-results && rimraf ./reports/allure-report && rimraf ./reports/logs",
    "copy-shared": "./copy-shared.sh",
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npx eslint --fix . --ext .ts",
    "prepare": "husky install",
    "postinstalla": "scripts/patch-test-driver.sh && npm run copy-shared && npm run merge-element-references",
    "merge-element-references": "json-concat shared/element-references/common-element-references.json src/element-references/des-element-references.json shared/element-references/combined-element-references.json",
    "compile": "npx tsc --noEmit -p .",
    "wdio": "wdio run ./wdio.conf.ts"
  },
  "dependencies": {
    "@cucumber/cucumber": "^9.0.1",
    "@dvsa/app-dev-automation": "0.0.38",
    "@wdio/cucumber-framework": "^8.6.2",
    "@wdio/types": "^8.4.0",
    "appium": "^2.0.0-beta.57",
    "appium-xcuitest-driver": "^4.19.1",
    "browserstack-local": "^1.5.1",
    "chromedriver": "^111.0.0",
    "date-fns": "^2.29.3",
    "json-concat": "0.0.1",
    "ms-chromium-edge-driver": "^0.5.1",
    "wdio-video-reporter": "^3.5.0",
    "webdriverio": "^8.6.3"
  },
  "devDependencies": {
    "@types/node": "18.11.x",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@wdio/allure-reporter": "^8.5.0",
    "@wdio/appium-service": "^8.6.3",
    "@wdio/browserstack-service": "^8.6.3",
    "@wdio/cli": "^8.6.5",
    "@wdio/config": "^8.6.2",
    "@wdio/local-runner": "^8.6.3",
    "@wdio/selenium-standalone-service": "^8.6.2",
    "@wdio/shared-store-service": "^8.6.3",
    "@wdio/spec-reporter": "^8.4.0",
    "@wdio/static-server-service": "^8.4.0",
    "allure-commandline": "^2.21.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-wdio": "^8.0.14",
    "exceljs": "^4.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "wdio-chromedriver-service": "^8.1.1"
  },
  "// devDependencies.wdio-cucumberjs-json-reporter": "TBC",
  "lint-staged": {
    "./src/**/*.ts": [
      "npm run lint:fix"
    ]
  }
}
